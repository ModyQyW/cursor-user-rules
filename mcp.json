{
  "mcpServers": {
    "exa": {
      "type": "http",
      "url": "https://mcp.exa.ai/mcp",
      "headers": {}
    },
    "context7": {
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "Sentry": {
      "url": "https://mcp.sentry.dev/mcp"
    },
    "chrome-devtools": {
      "command": "bunx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ]
    }
  }
}